[sub_resource type="GDScript" id="GDScript_tbdk2"]
script/source = "extends AnimatedSprite2D


const ROT = 0.5 # Rate of change
const S = 0.8
const L = 0.8

var hue = 0.0

@onready var gradient: Sprite2D = $Gradient


func _process(delta: float) -> void:
	var texture = gradient.texture as GradientTexture2D
	hue += delta * ROT
	for point in range(2):
		print(hue + (float(point) / 2))
		texture.gradient.set_color(point, Color.from_ok_hsl(hue + (float(point) / 2), S, L))
"

